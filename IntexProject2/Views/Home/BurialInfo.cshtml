        @if (ViewBag.Textile != null)
        {
            <h2>Textile Data for Selected Burial</h2><hr />
            <div class="grid-container">
                <span class="grid-item data-title">Locale: </span>
                <span class="grid-item">@(ViewBag.Textile.Locale != null ? ViewBag.Textile.Locale : "No Data")</span>

@{
}

<div>
    @if (ViewBag.Yarnmanipulation != null)
    {
        foreach (Yarnmanipulation y in ViewBag.Yarnmanipulation)
        {
            <div>
                <span>@y.Id</span>
                <span>@y.Direction</span>
            </div>
        }
    }
    else
    {
        <div>
            <span>No data to display</span>
        </div>
    }
</div>